<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Form.
    -->
    <changeSet id="20210105062359-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="form_type_id"
                                 baseTableName="form"
                                 constraintName="fk_form_form_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="form_type"/>

        <addForeignKeyConstraint baseColumnNames="employee_id"
                                 baseTableName="form"
                                 constraintName="fk_form_employee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

        <addForeignKeyConstraint baseColumnNames="equipment_id"
                                 baseTableName="form"
                                 constraintName="fk_form_equipment_id"
                                 referencedColumnNames="id"
                                 referencedTableName="equipment"/>

    </changeSet>
</databaseChangeLog>

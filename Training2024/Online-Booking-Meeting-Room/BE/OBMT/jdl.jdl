entity AppUsers {
  username String required
  password String required
  email String required
  phoneNumber String required
  resetToken String
  resetTokenCreatedAt ZonedDateTime
  otpCode String
  otpCodeCreatedAt ZonedDateTime
  otpCodeExpiredAt ZonedDateTime
  otpIsVerified Boolean required
  rememberToken String
  isRemembered Boolean required
  deviceInfo String
  createdAt ZonedDateTime required
  updatedAt ZonedDateTime required
  status String required
}
entity EventMeetings {
  title String required
  startTime ZonedDateTime required
  endTime ZonedDateTime required
  description String required
  createdAt ZonedDateTime required
  updatedAt ZonedDateTime required
  status String required
}
entity EventUsers {
  isOrganizer Boolean required
}
entity MeetingRooms {
  name String required
  description String required
  capacity Integer required
  createdAt ZonedDateTime required
  updatedAt ZonedDateTime required
}
entity Permissions {
  permissionName String required
}
entity Roles {
  roleName String required
}
relationship ManyToOne {
  EventMeetings{meetingRoom(name)} to MeetingRooms
  EventUsers{appusers(username)} to AppUsers
  EventUsers{eventMeeting(title)} to EventMeetings
}
relationship ManyToMany {
  AppUsers{roles(roleName)} to Roles{appusers(username)}
  AppUsers{events(title)} to EventMeetings{members(username)}
  Roles{permissions(permissionName)} to Permissions{roles(roleName)}
}

dto AppUsers, EventMeetings, EventUsers, MeetingRooms, Permissions, Roles with mapstruct
paginate AppUsers, EventMeetings, EventUsers, MeetingRooms, Permissions, Roles with pagination
service AppUsers, EventMeetings, EventUsers, MeetingRooms, Permissions, Roles with serviceImpl
